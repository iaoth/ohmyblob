function _draw()
	cls()
	if fi>0 then
		for c=1,15 do
			pal(c,fadeout[c][flr(fi)],1)
		end
	end
	map(0,0,0,0,16,16)

	if fi==0 then
		frame+=0.1
		if (frame>#fade) frame=1
		local tc=fade[flr(frame)]
		print("press any key",70,120,tc)
	end
