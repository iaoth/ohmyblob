function _init()
	music(0)
end

fi=0

function _update60()
	if fi>=5.9 then
		load("omb.p8","quit to title")
	elseif fi>0 then
		fi+=0.04
	elseif btn()!=0 then
		fi=1
		music(-1,1500)
	end
end

